     name : refreshes
     procedure_id : 1761972687844833627
     type_code : 14
     code : |1-
      // Summary: Refreshes the information
      
      PROCEDURE refreshes()
      
      
      HReadSeekFirst(gsQuerySource, PostID, gnID)
      
      
      // Variables
      sHTML         is string
      
      // Displays the information
      
      // Positions on the relevant post
      HReadSeekFirst(Post,PostID,gnID)
      IF HFound(Post) THEN
      	
      	// Post Title
      	STC_POST_TITLE = Post.Title
      	
      	WHILE TextHeight(STC_POST_TITLE,Post.Title,tMultiline) > STC_POST_TITLE..Height 
      		STC_POST_TITLE..FontSize -= 1
      	END
      	
      	dtTPostDate is datetime  = {gsQuerySource + ".PublicationDate",indItem}
      	STC_Date_time = DateToString(dtTPostDate..Date, "MM/DD/YYYY") + " at " + TimeToString(dtTPostDate..Time, "H:MM AP")
      	
      	
      	// Switches the post to read if it was unread
      	IF Post.Read = False THEN
      		Post.Read = True
      		HModify(Post)
      	END
      	
      	// Displaying the title
      	SWITCH gsWindowSource
      		CASE "WIN_PostList":
      			MyWindow..Title  = "All the posts "  + StringBuild("(%1/%2)", gnCurrentSubscript, gnTotal)
      		CASE "WIN_FavoritesList":
      			MyWindow..Title  = "All the favorites "  + StringBuild("(%1/%2)", gnCurrentSubscript, gnTotal)
      	END
      	
      	//If we have an image only
      	IF Post.Image <> "" AND Post.Description = "" THEN
      		MyWindow..Plane = 2
      		IMG_POSTOnly =  Post.Image
      	END
      	
      	//If we have text only
      	IF Post.Description <> "" AND Post.Image = "" THEN
      		MyWindow..Plane = 3
      		HTML_CONTENTOnly = postHTML(Post.Description)
      	END
      	
      	//If we have text and an image
      	IF Post.Description <> "" AND Post.Image <> "" THEN
      		MyWindow..Plane = 1
      		IMG_POST =  Post.Image
      		HTML_CONTENT = postHTML(Post.Description)
      	END
      	
      	//If no content >> display an info 	
      	IF Post.Image = "" AND Post.Description = "" THEN
      		MyWindow..Plane = 3
      		sHTML = "Click the 'Read next' button to go to the Web page."
      		HTML_CONTENTOnly = postHTML(sHTML)
      	END
      	
      	
      	// Enables/Disables the button according to the positioning
      	IF gnCurrentSubscript = 1 THEN
      		BTN_PREVIOUS..State = Grayed
      	ELSE
      		BTN_PREVIOUS..State = Active
      	END
      	
      	IF gnCurrentSubscript = gnTotal THEN
      		BTN_NEXT..State = Grayed
      	ELSE
      		BTN_NEXT..State = Active
      	END
      	
      	// This post has a link
      	IF Post.Link <> "" THEN
      		BTN_VIEW..State = Active
      	ELSE
      		BTN_VIEW..State = Grayed
      	END
      	
      	
      	// Favorites mode?
      	IF gsWindowSource = "WIN_FavoritesList" THEN
      		
      		IMG_FAVORITE..Visible = True
      		IMG_NOFAVORITE..Visible = False
      		
      		
      	ELSE
      		
      		// Favorite post?
      		IF Post.Favorites = True THEN
      			IMG_FAVORITE..Visible = True
      			IMG_NOFAVORITE..Visible = False
      		ELSE
      			IMG_NOFAVORITE..Visible = True 
      			IMG_FAVORITE..Visible = False
      			
      		END	
      	END
      ELSE
      	Error("Post not found")
      	Close()
      END
