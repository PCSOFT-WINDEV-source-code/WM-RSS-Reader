      PROCEDURE PostDelete(LOCAL nPostID is int)
      
      HReadSeekFirst(Post, PostID, nPostID)
      
      IF HFound(Post) THEN
      	
      	//Delete the image illustrating the post from disk
      	IF Post.Image <> "" THEN fDelete(Post.Image)
      	
      	HDelete(Post)
      	
      END
