     name : streamFlagPostsAsRead
     procedure_id : 1761972713619492817
     type_code : 15
     code : |1-
      PROCEDURE streamFlagPostsAsRead(LOCAL nStreamID is  int)
      
      
      HourGlass(True)
      
      FOR EACH Post WHERE RSSStreamID = nStreamID
      	Post.Read = True
      	HModify(Post)
      END
      
      
      END:
      HourGlass(False)
