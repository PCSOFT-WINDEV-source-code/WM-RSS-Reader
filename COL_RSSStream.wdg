      	CASE "YAKT"
      		duOffsetDuration = "0090000000"
      	CASE "YEKT"
      		duOffsetDuration = "0050000000"
      	CASE "Z"
      		duOffsetDuration = "0000000000"
      	OTHER CASE
      		duOffsetDuration = "0000000000"
      END
      
      
      
      RESULT duOffsetDuration
      
      
