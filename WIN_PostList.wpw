  procedures :
   -
     name : refreshList
     internal_properties : CAAAAAgAAABe73HobyQUgdKgNUzL0bdwwLt290KpPaDNrfQwfGloveoRMVub9SUwrez9nWRIFv8YyfWSinpy1WCGC++a3djTjgR4RVuZhIN8d93S9q3m+yjV8Gt0H6j5K0PQ1/YtToWQPWvqlTrj+g8SemGF
     procedure_id : 1761972692139998656
     type_code : 14
     code : |1+
      // Summary: Refreshes the list of posts
      
      PROCEDURE refreshList()
      
      nContentWidth is int  = STC_CONTENT..Width 
      
      LooperDeleteAll(LOOP_POSTS)
      
      // Positions on the stream
      HReadSeekFirst(RSSStream, RSSStreamID, gnRSSStreamID)
      
      IF HFound(RSSStream) THEN
      	
      	//Title of the window
      	WIN_PostList..Title = RSSStream.Title
      	
      	// Initialize the query to retrieve all the posts of the stream
      	QRY_PostList.pRSSStreamID = gnRSSStreamID
      	HExecuteQuery(QRY_PostList)
      	
      	FOR EACH QRY_PostList 
      		
      		nSubscript is int = LooperAddLine(LOOP_POSTS, QRY_PostList.PostID, QRY_PostList.Title,  QRY_PostList.Description, "",QRY_PostList.Image)	
      		
      		//**If there is no image to display, display the text on the entire width
      		
      		IF QRY_PostList.Image = "" THEN
      			LOOP_POSTS[nSubscript].IMG_ILLUS..X = LOOP_POSTS..Width - LOOP_POSTS..X 
      		ELSE
      			LOOP_POSTS[nSubscript].IMG_ILLUS..X = LOOP_POSTS..Width - LOOP_POSTS..X - IMG_ILLUS..Width - CoordinateEditorToScreen(10)
      		END
      		LOOP_POSTS[nSubscript].STC_CONTENT..Width = LOOP_POSTS[nSubscript].IMG_ILLUS..X - STC_CONTENT..X -  CoordinateEditorToScreen(10)
      		
      		//Captions used to calculate the text heights
      		STC_TestTitleHeight..Width =  LOOP_POSTS[nSubscript].IMG_ILLUS..X - STC_CONTENT..X -  CoordinateEditorToScreen(10)
      		STC_TestDescriptionHeight..Width =  LOOP_POSTS[nSubscript].IMG_ILLUS..X - STC_CONTENT..X -  CoordinateEditorToScreen(10)
      		
      		//**
      		
      		
      		//Date and time of the post
      		dtPublicationDate is datetime = QRY_PostList.PublicationDate
      		LOOP_POSTS.ATT_DATE_TIME[nSubscript] = DateToString(dtPublicationDate, maskDateRelativeDuration) 
      		
      		//Post already read?
      		IF QRY_PostList.Read = True THEN
      			LOOP_POSTS.ATT_CONTENT_COLOR[nSubscript] = DarkGray
      			LOOP_POSTS.ATT_TITLE_COLOR[nSubscript] = DarkGray
      			IF QRY_PostList.Image <> "" THEN 
      				imagePost is an Image = LOOP_POSTS.ATT_IMAGE[nSubscript]
      				dModifySaturation(imagePost, -50)
      				bufImage is buffer = imagePost
      				LOOP_POSTS.ATT_IMAGE[nSubscript] = bufImage
      			END
      		ELSE
      			LOOP_POSTS.ATT_CONTENT_COLOR[nSubscript] = RGB(101,128,146)
      		END
      		
      		
      		//**Title and description of the post = contenu of the post
      		//First, display the entire title with the biggest font
      		//Then, if possible, display a little bit of the post description that will be truncated according to the remaining space
      		
      		sContent is string 
      		IF LOOP_POSTS.ATT_Description[nSubscript] =  "" THEN
      			
      			//If no description, display the title only 
      			sContent = gPen(RGB(51,51,51)) + gFontSize(5) + LOOP_POSTS.ATT_TITLE[nSubscript]
      			
      		ELSE
      			
      			//If a description is found
      			//Define the number of pixels in height required to display the title
      			nTitleHeight is int = TextHeight(STC_TestTitleHeight,  LOOP_POSTS.ATT_TITLE[nSubscript] + CR , tMultiline)
      			//Deduce the remaining height available for the post description
      			nRemainingHeightForDescr is int = STC_CONTENT..Height - nTitleHeight
      			
      			sDescription is string = formatContent(LOOP_POSTS.ATT_Description[nSubscript]) 
      			
      			n is int = 1
      			LOOP
      				
      				//As long as the description is too big in relation to the available space, truncate
      				nDescriptionHeight is int = TextHeight(STC_TestDescriptionHeight, sDescription+["... "], tMultiline)
      				IF nDescriptionHeight >= nRemainingHeightForDescr THEN
      										
      					//Truncate a first time by being based on a product in cross
      					nNbPossibleCharact is int = (nRemainingHeightForDescr * Length(sDescription)) / nDescriptionHeight
      					sDescription = Truncate(sDescription, Length(sDescription)-nNbPossibleCharact, numberOfCharactersToDelete) 
      					//					
      					//truncate on an entire word
      					sLastWord is string  = ExtractString(NoSpace(sDescription), lastRank, " ", FromEnd)
      					IF length(sLastWord) >= Length(sDescription) THEN BREAK
      					sDescription = Truncate(sDescription, Length(sLastWord)+1, numberOfCharactersToDelete) 
      					IF sDescription = EOT THEN BREAK			
      					
      				ELSE 
      					BREAK
      				END
      				
      			END
      			
      			
      			
      			//If description is found after the truncation, use "..." to specify that it was truncated
      			IF sDescription <> EOT THEN
      				sDescription += ["..."]
      				sContent = gPen(RGB(51,51,51)) + gFontSize(5) + LOOP_POSTS.ATT_TITLE[nSubscript]+ CR + gPen(RGB(101,128,146)) + gFontSize(4) + sDescription
      			ELSE
      				sContent = gPen(RGB(51,51,51)) + gFontSize(5) + LOOP_POSTS.ATT_TITLE[nSubscript]
      			END
      		END
      		
      		LOOP_POSTS.ATT_CONTENT[nSubscript]  = sContent
      		
      		//**
      		
      		
      	END
      	
      	
      	IF LOOP_POSTS..Occurrence = 0 THEN
      		Close()
      	END
      END
