      // Refreshes the list of posts
      FOR ALL ROW nSubscript of LOOP_FAVORITES
      	
      	IF ATT_IMAGE[nSubscript] = "" THEN
      		LOOP_FAVORITES[nSubscript].IMG_ILLUS..X = LOOP_FAVORITES..Width - LOOP_FAVORITES..X
      	ELSE
      		LOOP_FAVORITES[nSubscript].IMG_ILLUS..X = LOOP_FAVORITES..Width - LOOP_FAVORITES..X - IMG_ILLUS..Width - CoordinateEditorToScreen(10)
      	END
      	
      	LOOP_FAVORITES[nSubscript].STC_TITLE..Width = LOOP_FAVORITES[nSubscript].IMG_ILLUS..X - STC_TITLE..X - CoordinateEditorToScreen(10)
      	LOOP_FAVORITES[nSubscript].STC_CONTENT..Width = LOOP_FAVORITES[nSubscript].IMG_ILLUS..X - STC_CONTENT..X - CoordinateEditorToScreen(10)
      	
      END
