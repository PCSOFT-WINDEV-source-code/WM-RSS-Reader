  procedures :
   -
     name : refreshes
     procedure_id : 1761972696435163669
     type_code : 14
     code : |1+
      // Summary: Refreshes the list
      
      PROCEDURE refreshes()
      
      LooperDeleteAll(LOOP_FAVORITES)
      
      // Parameters of the 'QRY_PostListFavorites' query
      QRY_PostListFavorite.pFavorites = True
      HExecuteQuery(QRY_PostListFavorite)
      
      FOR EACH QRY_PostListFavorite 
      	
      	// Date & Time
      	sDateTime is string = StringBuild("%1 at %2",DateToString(QRY_PostListFavorite.PublicationDate..Date,"MM/DD/YYYY"),TimeToString(QRY_PostListFavorite.PublicationDate..Time, "H:MM AP"))
      	
      	sContent is string 
      	IF QRY_PostListFavorite.Description ~= "" THEN
      		sContent = "<No content>"
      	ELSE
      		sContent= formatContent(QRY_PostListFavorite.Description)
      	END
      	 
      	nSubscript is int = LooperAddLine(LOOP_FAVORITES, QRY_PostListFavorite.PostID, QRY_PostListFavorite.Image, sDateTime, QRY_PostListFavorite.Title, sContent)
      	
      	//If there is no image to display, display the text on the entire width
      	IF QRY_PostListFavorite.Image = "" THEN
      		LOOP_FAVORITES[nSubscript].IMG_ILLUS..X = LOOP_FAVORITES..Width - LOOP_FAVORITES..X 
      	ELSE
      		LOOP_FAVORITES[nSubscript].IMG_ILLUS..X = LOOP_FAVORITES..Width - LOOP_FAVORITES..X - IMG_ILLUS..Width - CoordinateEditorToScreen(10)
      	END
      	LOOP_FAVORITES[nSubscript].STC_TITLE..Width = LOOP_FAVORITES[nSubscript].IMG_ILLUS..X - STC_TITLE..X - CoordinateEditorToScreen(10)
      	LOOP_FAVORITES[nSubscript].STC_CONTENT..Width = LOOP_FAVORITES[nSubscript].IMG_ILLUS..X - STC_CONTENT..X -  CoordinateEditorToScreen(10)
      	
      	
      END
      
